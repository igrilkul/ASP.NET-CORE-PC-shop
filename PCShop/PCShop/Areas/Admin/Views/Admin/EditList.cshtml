@model IEnumerable<ProductListViewModel>

@{
    ViewData["Title"] = "Edit List";
}



@if (!Model.Any())
{
    <h2 class="text-center">No Items found.</h2>
}

@if (Model.Any())
{
    <ul class="list-group list-group-flush">
        @foreach (var item in Model)
        {
            <partial name="_ListViewPartial" model="item" />
        }
    </ul>
}